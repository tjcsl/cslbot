[core]
host: irc.freenode.net
serverport: 2688
ircport: 6667
ssl: False
ipv6: False
sasl: False
nick: exampleBot
channel: #channel
ctrlchan: #channel-control
extrachans:
cmdchar: !
altcmdchars: fwil, @, ~
url: http://example.com/irc/
nickregex: [a-zA-Z0-9_\|`\^-]
chanregex: [&#+!][^, \x07]+
extramodules:
[groups]
hooks: core, optional
commands: admin, core, useful, optional
[auth]
ctrlpass:
serverpass:
ctrlkey:
owner:
[db]
engine:
geoip: static/geoip.db
[alembic]
script_location: cslbot/alembic
[feature]
nickserv: True
server: True
prettyerrors: True
lazyregex: True
hooks: True
capskick: True
nickkick: True
linkread: True
voiceactive: True
networktype: atheme
servicestype: atheme
[api]
weatherapikey:
ebayapikey:
githubapikey:
githubrepo:
wolframapikey:
fmlkey:
googleapikey:
xkcdsearchid:
googlesearchid:
stands4uid:
stands4token:
tumblrconsumerkey:
tumblrconsumersecret:
tumblroauthkey:
tumblroauthsecret:
