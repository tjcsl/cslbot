[core]
host: irc.freenode.net
serverport: 2688
ircport: 6667
nick: exampleBot
channel: #channel
ctrlchan: #channel-control
extrachans: #chan1, #chan2
cmdchar: !
altcmdchars: fwil, @, ~
url: http://example.com/irc/
nickregex: [a-zA-Z0-9_\|`\^-]
[auth]
ctrlpass:
nickpass:
ctrlkey:
admins: admin1, admin2
[feature]
stallmanmode = False
clippy = False
lazyregex = True
hooks = True
capskick = True
linkread = True
[wiki]
list = wiki, livedoc
api = http://en.wikipedia.org/w/api.php, http://www.tjhsst.edu/admin/livedoc/api.php
article_url = http://en.wikipedia.org/wiki/, http://www.tjhsst.edu/admin/livedoc/
names = Wikipedia, LiveDoc
default = wiki
[api]
steamapikey:
dictionaryapikey:
weatherapikey:
ebayapikey:
githubapikey:
githubrepo:
wolframapikey:
translateid:
translatesecret:
fmlkey:
xkcdkey:
xkcdid:
