[core]
host: irc.freenode.net
serverport: 2688
ircport: 6667
ssl: False
ipv6: False
sasl: False
nick: exampleBot
channel: #channel
ctrlchan: #channel-control
extrachans:
cmdchar: !
altcmdchars: fwil, @, ~
url: http://example.com/irc/
nickregex: [a-zA-Z0-9_\|`\^-]
chanregex: [#&+!][a-zA-Z0-9_-!#]*
extracommands:
[groups]
hooks: core, optional
commands: admin, core, useful, optional
[auth]
ctrlpass:
nickpass:
serverpass:
ctrlkey:
admins: admin1, admin2
[db]
engine:
geoip: static/geoip.db
[feature]
nickserv: True
server: True
prettyerrors: True
lazyregex: True
hooks: True
capskick: True
nickkick: True
linkread: True
[api]
steamapikey:
dictionaryapikey:
weatherapikey:
ebayapikey:
githubapikey:
githubrepo:
wolframapikey:
translateid:
translatesecret:
